    <fields>
        <fullName>Locked__c</fullName>
        <description>1 if current time is greater than game start time.</description>
        <externalId>false</externalId>
        <formula>IF ( NOW() &gt;  Start_Time__c, 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>1 if current time is greater than game start time.</inlineHelpText>
        <label>Locked</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Points__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Winner__c), 0.0, IF(Winner__c == &apos;PUSH&apos;, 0.5, 1.0))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Points</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Spread__c</fullName>
        <description>Point spread with respect to home team. Negative number indicates home team is favorite. Positive number indicates visiting team is favorite. Zero indicates a pick&apos;em game.</description>
        <externalId>false</externalId>
        <label>Spread</label>
        <precision>4</precision>
        <required>false</required>
        <scale>1</scale>
        <trackTrending>false</trackTrending>
