<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Current_Week</fullName>
    <active>true</active>
    <errorConditionFormula>OR(CurrentWeek__c &lt; 1,  CurrentWeek__c &gt; 18)</errorConditionFormula>
    <errorDisplayField>CurrentWeek__c</errorDisplayField>
    <errorMessage>The current week must be a number between 1 and 18 inclusive.</errorMessage>
</ValidationRule>
